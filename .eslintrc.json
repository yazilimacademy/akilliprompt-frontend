{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-unused-vars": [
      "error"
    ],
    "no-redeclare": [
      "error"
    ],
    "camelcase": [
      "error",
      {
        "properties": "always"
      }
    ],
    "no-var": [
      "error"
    ],
    "indent": [
      "error",
      2
    ],
    "no-console": "warn"
  }
}
